{
  "name": "motto-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "detox test --configuration ios.sim.debug --headless",
    "build:e2e": "expo prebuild && expo run:ios --device 'iPhone 14' --no-install",
    "build:e2e:android": "expo prebuild && expo run:android --variant debug --no-install",
    "test:e2e:android": "detox test --configuration android.emu.debug --headless"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@testing-library/react-native": "^13.2.0",
    "expo": "~50.0.0",
    "expo-app-loading": "~2.1.1",
    "expo-application": "~5.8.3",
    "expo-asset": "~9.0.2",
    "expo-auth-session": "~5.4.0",
    "expo-av": "~13.10.6",
    "expo-background-fetch": "~11.8.1",
    "expo-barcode-scanner": "~12.9.2",
    "expo-battery": "~7.7.2",
    "expo-blur": "~12.9.1",
    "expo-brightness": "11.8.0",
    "expo-build-properties": "~0.11.1",
    "expo-calendar": "12.2.1",
    "expo-constants": "~15.4.5",
    "expo-contacts": "~12.8.1",
    "expo-crypto": "12.8.1",
    "expo-device": "~5.9.3",
    "expo-document-picker": "~11.10.1",
    "expo-face-detector": "~12.6.1",
    "expo-file-system": "~16.0.9",
    "expo-font": "~11.10.2",
    "expo-gl": "~13.6.0",
    "expo-haptics": "~12.8.1",
    "expo-image": "~1.10.6",
    "expo-image-manipulator": "~11.8.0",
    "expo-image-picker": "~14.7.1",
    "expo-keep-awake": "~12.8.1",
    "expo-linear-gradient": "~12.7.1",
    "expo-linking": "~6.2.2",
    "expo-local-authentication": "~13.8.0",
    "expo-location": "~16.5.2",
    "expo-mail-composer": "12.7.1",
    "expo-media-library": "~15.9.1",
    "expo-network": "~5.8.0",
    "expo-notifications": "~0.27.6",
    "expo-print": "~12.8.1",
    "expo-random": "~13.6.0",
    "expo-screen-orientation": "~6.4.0",
    "expo-secure-store": "~12.8.1",
    "expo-sensors": "~12.9.1",
    "expo-sharing": "~11.10.0",
    "expo-sms": "11.7.1",
    "expo-speech": "~11.7.0",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "~11.7.1",
    "expo-updates": "~0.24.9",
    "expo-web-browser": "~12.8.2",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "lottie-react-native": "6.5.1",
    "react": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-localize": "^3.5.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",

    "react-native-tts": "^4.1.1",
    "react-native-view-shot": "3.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/react": "~18.2.45",
    "detox": "^20.12.0",
    "expo-cli": "^6.3.10",
    "expo-detox-hook": "1.0.10",
    "jest": "^30.0.0",
    "jest-circus": "^29.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 14"
        },
        "binaryPath": "bin/Exponent.app"
      },
      "android.emu.debug": {
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_5_API_33"
        },
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk"
      }
    }
  },
  "resolutions": {
    "react": "18.2.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-navigation)"
    ],
    "moduleNameMapper": {
      "\\.(mp3|wav|ogg|m4a)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
